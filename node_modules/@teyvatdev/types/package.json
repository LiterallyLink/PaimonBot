{
  "name": "@teyvatdev/types",
  "description": "Helpful node types for developing with the Teyvat Dev API",
  "types": "dist/index.d.ts",
  "version": "0.4.6",
  "scripts": {
    "schema-gen": "ts-node ./schema-gen/generate.ts",
    "generate": "prisma generate && sh -c ./scripts/copyAndParseSrc.sh",
    "build": "tsc --emitDeclarationOnly && sh -c ./scripts/copyDist.sh",
    "clean": "rm -rf node_modules src/types dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teyvat-dev/types.git"
  },
  "keywords": [
    "genshin-impact",
    "teyvat-dev",
    "types",
    "api"
  ],
  "author": "Drew Hutton <Yoroshi>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/teyvat-dev/types/issues"
  },
  "homepage": "https://github.com/teyvat-dev/types.git",
  "devDependencies": {
    "@prisma/cli": "2.14.0",
    "@prisma/client": "2.14.0",
    "@types/node": "^14.14.12",
    "mkdirp": "^1.0.4",
    "ts-node": "^9.1.1",
    "typedoc": "^0.19.2",
    "typescript": "^4.1.0"
  },
  "files": [
    "dist"
  ]
}
